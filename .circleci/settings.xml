<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
          xmlns="http://maven.apache.org/SETTINGS/1.1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <profiles>
    <profile>
      <id>vertispan-artifactory</id>
      <activation>
        <property>
          <name>vertispanArtifactory</name>
        </property>
      </activation>
      <repositories>
        <repository>
          <snapshots/>
          <id>vertispan-gwtproject</id>
          <name>gwt-snapshot</name>
          <url>https://repo.vertispan.com/gwt-snapshot</url>
        </repository>
      </repositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>vertispan-artifactory</activeProfile>
  </activeProfiles>
</settings>
